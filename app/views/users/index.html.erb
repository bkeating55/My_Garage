<h1>Listing Users</h1>
<%= link_to 'New User', new_user_path %>
<table>
  <tr>
    <th>Name </th>

  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.user %></td>
      <td><%= link_to 'View', user_path(user) %></td>
      <td><%= link_to 'Edit User', edit_user_path(user) %></td>
      <td><%= link_to 'Delete User', user_path(user),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
